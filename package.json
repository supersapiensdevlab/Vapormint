{
  "name": "devvapormint",
  "version": "1.2.0",
  "description": "",
  "main": "dist/cjs/index.js",
  "module": "dist/devvapormint.esm.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build && yarn build-tailwind",
    "test": "tsdx test --passWithNoTests",
    "lint": "tsdx lint",
    "prepare": "yarn build",
    "size": "size-limit",
    "analyze": "size-limit --why",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "build-tailwind": "NODE_ENV=production npx tailwindcss -o ./dist/tailwind.css --minify",
    "buildtsc": "rm -rf dist/ &&  npm run build:esm && npm run build:cjs ",
    "build:esm": "tsc",
    "build:cjs": "tsc --module CommonJS --outDir dist/cjs"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/uibymaverick/testvapormint.git"
  },
  "size-limit": [
    {
      "path": "dist/devvapormint.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/devvapormint.esm.js",
      "limit": "10 KB"
    }
  ],
  "keywords": [
    "design system",
    "vapormint",
    "mintflick",
    "typescript componant package"
  ],
  "author": "maverick",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "7.14.6",
    "@heroicons/react": "2.0.17",
    "@mantine/hooks": "6.0.9",
    "@size-limit/preset-small-lib": "5.0.1",
    "@storybook/addon-essentials": "6.3.4",
    "@storybook/addon-info": "5.3.21",
    "@storybook/addon-links": "6.3.4",
    "@storybook/addon-postcss": "2.0.0",
    "@storybook/addons": "6.3.4",
    "@storybook/react": "6.3.4",
    "@tailwindcss/postcss7-compat": "2.2.4",
    "@types/node": "18.16.0",
    "@types/react": "18.0.38",
    "@types/react-dom": "18.0.11",
    "autoprefixer": "10.4.14",
    "babel-loader": "8.2.2",
    "husky": "7.0.1",
    "next": "13.3.1",
    "postcss": "8.4.23",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-easy-crop": "4.7.4",
    "react-is": "17.0.2",
    "react-player": "2.12.0",
    "size-limit": "5.0.1",
    "tailwindcss": "3.3.1",
    "tsdx": "0.14.1",
    "tslib": "2.3.0",
    "typescript": "5.0.4"
  },
  "peerDependencies": {
    "next": "13.3.1"
  }
}
